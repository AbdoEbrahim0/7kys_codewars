#include <string>
using namespace std;
std::string remove_vowels(const std::string& str) {
    string res = "";
    for (char c :str)
    switch (c)
    {
    case 'a' : case 'o': case 'e': case 'i': case 'u':
    case 'A': case 'E': case 'I': case 'O': case 'U':  
        break;
    default:
        res += c;
    }
    return res;
}
