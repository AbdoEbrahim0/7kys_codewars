#include <string>
using namespace std;
bool am_i_afraid(const std::string &day, int num)
{
    cout<< day <<"  "<<num<<endl;
 if ( (day=="Monday" && num==12)  || (day=="Wednesday" && num==34) ||
     (day=="Thursday" && num==0)  || (day=="Saturday" && num==56) ||
     (day=="Sunday" && num==666) || (day=="Sunday" && num==-666 )  ||
     (day=="Friday" && num%2==0) || (day=="Tuesday" && num>95) )
     return true;
     
    return false;
}
