 using namespace std;
    //method 1:
int repeats(std::vector<int>v) {
    //...[4,5,7,5,4,8]
    int sum = 0, times = 0;

    for (int i = 0; i < v.size(); i++)
    {
        for (int j = 0; j < v.size(); j++)
            if (v.at(i) == v.at(j)) times++;
        if (times == 1) sum += v.at(i);
        times = 0;
    }
    return sum;
}
